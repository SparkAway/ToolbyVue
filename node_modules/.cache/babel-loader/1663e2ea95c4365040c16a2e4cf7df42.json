b,MAAT,EAAoB;AACrCf,IAAAA,KAAK,CAACyB,GAAN,CAAUC,GAAV,EAAc;AACZC,MAAAA,MAAM,EAACA;AADK,KAAd,EAGGE,IAHH,CAGQ,UAAAZ,QAAQ,EAAI;AAChBW,MAAAA,OAAO,CAACX,QAAQ,CAACR,IAAV,CAAP;AACD,KALH,EAMGqB,KANH,CAMS,UAAAd,GAAG,EAAI;AACZD,MAAAA,MAAM,CAACC,GAAD,CAAN;AACD,KARH;AASD,GAVM,CAAP;AAWD,C,CACD;;AACA,OAAO,SAASe,IAAT,CAAcL,GAAd,EAA4B;AAAA,MAAVjB,IAAU,uEAAH,EAAG;AACjC,SAAO,IAAIK,OAAJ,CAAY,UAACc,OAAD,EAASb,MAAT,EAAoB;AACrCf,IAAAA,KAAK,CAAC+B,IAAN,CAAWL,GAAX,EAAejB,IAAf,EACGoB,IADH,CACQ,UAAAZ,QAAQ,EAAI;AAChBW,MAAAA,OAAO,CAACX,QAAQ,CAACR,IAAV,CAAP;AACD,KAHH,EAGI,UAAAO,GAAG,EAAI;AACPD,MAAAA,MAAM,CAACC,GAAD,CAAN;AACD,KALH;AAMD,GAPM,CAAP;AAQD,C,CACD;;AACA,gBAAsBgB,QAAtB;AAAA;AAAA,C,CAKA;;;sEALO,iBAAwBN,GAAxB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAA4BC,YAAAA,MAA5B,2DAAmC,EAAnC;AAAA;AAAA,mBACS3B,KAAK,CAACyB,GAAN,CAAUC,GAAV,EAAc;AAACC,cAAAA,MAAM,EAACA;AAAR,aAAd,CADT;;AAAA;AACDM,YAAAA,GADC;AAELA,YAAAA,GAAG,GAACA,GAAG,CAACxB,IAAR;AAFK,6CAGEwB,GAHF;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,G;;;;AAMP,gBAAsBC,SAAtB;AAAA;AAAA;;;uEAAO,kBAAyBR,GAAzB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAA6BjB,YAAAA,IAA7B,8DAAkC,EAAlC;AAAA;AAAA,mBACST,KAAK,CAAC+B,IAAN,CAAWL,GAAX,EAAejB,IAAf,CADT;;AAAA;AACDwB,YAAAA,GADC;AAELA,YAAAA,GAAG,GAACA,GAAG,CAACxB,IAAR;AAFK,8CAGEwB,GAHF;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,G","sourcesContent":["import axios from 'axios';\r\nimport router from '@/router'\r\naxios.defaults.timeout = 5000;\r\naxios.defaults.baseURL ='';\r\n//http request 拦截器\r\naxios.interceptors.request.use(\r\n  config => {\r\n    // const token = getCookie('名称');注意使用的时候需要引入cookie方法，推荐js-cookie\r\n    config.data = JSON.stringify(config.data);\r\n    config.headers = {\r\n      'Content-Type':'application/x-www-form-urlencoded'\r\n    }\r\n    // if(token){\r\n    //   config.params = {'token':token}\r\n    // }\r\n    return config;\r\n  },\r\n  error => {\r\n    return Promise.reject(err);\r\n  }\r\n);\r\n\r\n//http response 拦截器\r\naxios.interceptors.response.use(\r\n  response => {\r\n    if(response.data.errCode ==2){\r\n      router.push({\r\n        path:\"/login\",\r\n        querry:{redirect:router.currentRoute.fullPath}//从哪个页面跳转\r\n      })\r\n    }\r\n    return response;\r\n  },\r\n  error => {\r\n    return Promise.reject(error)\r\n  }\r\n)\r\n\r\n//  封装get方法,返回promise对象\r\nexport function get(url,params={}){\r\n  return new Promise((resolve,reject) => {\r\n    axios.get(url,{\r\n      params:params\r\n    })\r\n      .then(response => {\r\n        resolve(response.data);\r\n      })\r\n      .catch(err => {\r\n        reject(err)\r\n      })\r\n  })\r\n}\r\n// 封装post请求\r\nexport function post(url,data = {}){\r\n  return new Promise((resolve,reject) => {\r\n    axios.post(url,data)\r\n      .then(response => {\r\n        resolve(response.data);\r\n      },err => {\r\n        reject(err)\r\n      })\r\n  })\r\n}\r\n// 封装getAwait方法,返回async方法\r\nexport async function getAwait(url,params={}){\r\n  let res=await axios.get(url,{params:params})\r\n  res=res.data\r\n  return res\r\n}\r\n// 封装getAwait方法,返回async方法\r\nexport async function postAwait(url,data={}){\r\n  let res=await axios.post(url,data)\r\n  res=res.data\r\n  return res\r\n}\r\n"]}]}